        <DATATYPE-DEFINITION-XHTML IDENTIFIER="dt-xhtml-001" LONG-NAME="XHTML Type"/>
      </DATATYPES>
      
      <!-- Spec Types -->
      <SPEC-TYPES>
        <SPEC-OBJECT-TYPE IDENTIFIER="sot-requirement" LONG-NAME="Requirement">
